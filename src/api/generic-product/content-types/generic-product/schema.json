{
  "kind": "collectionType",
  "collectionName": "generic_products",
  "info": {
    "singularName": "generic-product",
    "pluralName": "generic-products",
    "displayName": "Generic Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "weight_list": {
      "type": "customField",
      "options": [
        "250g",
        "500g",
        "1kg",
        "75cl",
        "1L"
      ],
      "customField": "plugin::multi-select.multi-select",
      "required": false
    },
    "brands": {
      "type": "customField",
      "options": [
        "Nestlé",
        "Coca-Cola",
        "Orangina",
        ""
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "seasons": {
      "type": "customField",
      "options": [
        "Janvier",
        "Février",
        "Mars",
        "Avril",
        "Mai",
        "Juin",
        "Juillet",
        "Août",
        "Septembre",
        "Octobre",
        "Novembre",
        "Décembre"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "estimated_consumption_date": {
      "type": "integer",
      "min": 0
    },
    "footprint": {
      "type": "float"
    },
    "storage_method": {
      "type": "customField",
      "options": [
        "Température ambiante",
        "Congélateur",
        "Réfrigérateur"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "generic_search_recommandation": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::generic-search-recommandation.generic-search-recommandation",
      "mappedBy": "generic_product"
    },
    "subcategory": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::generic-subcategory.generic-subcategory",
      "inversedBy": "generic_products"
    },
    "is_visible": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "weight_unit_default": {
      "type": "enumeration",
      "enum": [
        "g",
        "mL"
      ],
      "required": true,
      "default": "g"
    },
    "weight_default": {
      "type": "integer",
      "required": true,
      "default": 1
    }
  }
}
