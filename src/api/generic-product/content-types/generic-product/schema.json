    "footprint": {
      "type": "float"
    },
    "storage_method": {
      "type": "customField",
      "options": [
        "ambient",
        "fridge",
        "freezer"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "subcategory": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::generic-subcategory.generic-subcategory",
      "inversedBy": "generic_products"
    },
    "is_live": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "tags_list": {
      "type": "customField",
      "options": [
        "organic",
        "french"
      ],
      "required": true,
      "customField": "plugin::multi-select.multi-select"
    },
    "brands": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::brand.brand"
    },
    "weight_unit_default": {
      "type": "enumeration",
      "enum": [
        "g",
        "ml"
      ],
      "required": true
    },
    "weight_per_unit": {
      "type": "integer"
    },
    "matching_tags": {
      "type": "json"
    },
    "min_expiration_date": {
      "type": "integer",
      "min": 0
    },
    "max_expiration_date": {
      "type": "integer"
    },
    "unit_name": {
      "type": "string",
      "default": "La pi√®ce"
    },
    "is_unit": {
      "type": "boolean",
      "default": false
    },
    "pef_score": {
      "type": "decimal"
    },
    "synchronisation_ulid": {
      "type": "customField",
      "customField": "plugin::field-ulid.ulid",
      "required": true
    },
    "is_grouped": {
      "type": "boolean",
      "default": false
