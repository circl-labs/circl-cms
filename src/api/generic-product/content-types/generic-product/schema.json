{
  "kind": "collectionType",
  "collectionName": "generic_products",
  "info": {
    "singularName": "generic-product",
    "pluralName": "generic-products",
    "displayName": "Generic Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "import-export-entries": {
      "idField": "synchronisation_ulid"
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
