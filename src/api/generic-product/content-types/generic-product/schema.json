{
  "kind": "collectionType",
  "collectionName": "generic_products",
  "info": {
    "singularName": "generic-product",
    "pluralName": "generic-products",
    "displayName": "Generic Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "import-export-entries": {
      "idField": "synchronisation_ulid"
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "weight_list": {
      "type": "customField",
      "options": [
        "1",
        "2",
        "3",
        "4",
        "5",
        "6",
        "8",
        "10",
        "12",
        "18",
        "20",
        "25",
        "50",
        "100",
        "125",
        "150",
        "180",
        "200",
        "250",
        "300",
        "340",
        "350",
        "400",
        "500",
        "700",
        "750",
        "825",
        "1000",
        "1500",
        "2000",
        "10000"
      ],
      "customField": "plugin::multi-select.multi-select",
      "required": false
    },
    "seasons": {
      "type": "customField",
      "options": [
        "january",
        "february",
        "march",
        "april",
        "may",
        "june",
        "july",
        "august",
        "september",
        "october",
        "november",
        "december"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "footprint": {
      "type": "float"
    },
    "storage_method": {
      "type": "customField",
      "options": [
        "ambient",
        "fridge",
        "freezer"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "subcategory": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::generic-subcategory.generic-subcategory",
      "inversedBy": "generic_products"
    },
    "is_visible": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "tags_list": {
      "type": "customField",
      "options": [
        "organic",
        "french"
      ],
      "required": true,
      "customField": "plugin::multi-select.multi-select"
    },
    "brands": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::brand.brand"
    },
    "weight_unit_default": {
      "type": "enumeration",
      "enum": [
        "g",
        "ml",
        "u"
      ],
      "required": true
    },
    "weight_per_unit": {
      "type": "integer"
    },
    "matching_tags": {
      "type": "json"
    },
    "min_expiration_date": {
      "type": "integer",
      "min": 0
    },
    "max_expiration_date": {
      "type": "integer"
    },
    "unit_name": {
      "type": "string",
      "default": "La pi√®ce"
    },
    "is_unit": {
      "type": "boolean",
      "default": false
    },
    "pef_score": {
      "type": "decimal"
    },
    "synchronisation_ulid": {
      "type": "customField",
      "customField": "plugin::field-ulid.ulid"
    }
  }
}
