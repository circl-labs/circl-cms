{
  "kind": "collectionType",
  "collectionName": "generic_subcategories",
  "info": {
    "singularName": "generic-subcategory",
    "pluralName": "generic-subcategories",
    "displayName": "Generic Subcategory",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
