{
  "kind": "collectionType",
  "collectionName": "seller_shops",
  "info": {
    "singularName": "seller-shop",
    "pluralName": "seller-shops",
    "displayName": "Seller Shop",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "import-export-entries": {
      "idField": "name"
    }
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "opening_hours": {
      "type": "string"
    },
    "str_acc_id": {
      "type": "string",
      "unique": true,
      "required": false
    },
    "seller": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::seller.seller",
      "inversedBy": "seller_shops"
    },
    "address": {
      "type": "component",
      "repeatable": false,
      "component": "address.address"
    },
    "email": {
      "type": "email"
    },
    "phone_number": {
      "type": "biginteger"
    }
  }
}
