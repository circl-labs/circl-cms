{
  "kind": "collectionType",
  "collectionName": "ordered_items",
  "info": {
    "singularName": "ordered-item",
    "pluralName": "ordered-items",
    "displayName": "Ordered Item",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "quantity": {
      "type": "integer",
      "required": true
    },
    "weight": {
      "type": "decimal"
    },
    "weight_unit": {
      "type": "enumeration",
      "enum": [
        "g",
        "ml",
        "u"
      ],
      "required": true
    },
    "thrown": {
      "type": "decimal"
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "order": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::order.order"
    },
    "price_by_weight": {
      "type": "decimal"
    },
    "expiration_date": {
      "type": "datetime"
    },
    "tags_list": {
      "type": "customField",
      "options": [
        "organic",
        "french"
      ],
      "customField": "plugin::multi-select.multi-select"
    }
  }
}
