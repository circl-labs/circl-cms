{
  "kind": "collectionType",
  "collectionName": "sellers",
  "info": {
    "singularName": "seller",
    "pluralName": "sellers",
    "displayName": "Seller",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "import-export-entries": {
      "idField": "synchronisation_ulid"
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "delivery_threshold": {
      "type": "integer"
    },
    "free_delivery_threshold": {
      "type": "integer"
    },
    "delivery": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "click_and_collect": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "is_partner": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "website_url": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "delivery_time": {
      "type": "integer",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "seller_shops": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::seller-shop.seller-shop",
      "mappedBy": "seller"
    },
    "seller_products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::seller-product.seller-product"
    },
    "prefilled_basket_url": {
      "type": "string"
    },
    "click_and_collect_threshold": {
      "type": "integer"
    },
    "min_cost_delivery": {
      "type": "decimal"
    },
    "max_cost_delivery": {
      "type": "decimal"
    },
    "click_and_collect_time": {
      "type": "integer"
    },
    "delivery_area": {
      "type": "customField",
      "options": [
        "Paris:75",
        "Hauts-de-Seine:92",
        "Val-de-Marne:94",
        "Seine-Saint-Denis:93",
        "Paris 8:75008",
        "Paris 16:75016",
        "Paris 116:75116",
        "Paris 17:75017",
        "Boulogne-Billancourt:92100",
        "Neuilly-sur-Seine:92200",
        "Puteaux:92800"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "integration_type": {
      "type": "enumeration",
      "enum": [
        "WOO_COMMERCE"
      ]
    },
    "integration_config": {
      "type": "json"
    },
    "synchronisation_ulid": {
      "type": "customField",
      "customField": "plugin::field-ulid.ulid",
      "required": true
    },
    "promotion_code": {
      "type": "string"
    }
  }
}
