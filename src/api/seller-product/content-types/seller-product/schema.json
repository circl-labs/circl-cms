{
  "kind": "collectionType",
  "collectionName": "seller_products",
  "info": {
    "singularName": "seller-product",
    "pluralName": "seller-products",
    "displayName": "Seller Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "category": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "text"
    },
    "origin": {
      "type": "string"
    },
    "weight_unit": {
      "type": "string"
    },
    "tags_list": {
      "type": "customField",
      "options": [
        "french",
        "organic"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "brand": {
      "type": "string"
    },
    "nutriscore": {
      "type": "enumeration",
      "enum": [
        "A",
        "B",
        "C",
        "D",
        "E"
      ]
    },
    "ecoscore": {
      "type": "enumeration",
      "enum": [
        "A",
        "B",
        "C",
        "D",
        "E"
      ]
    },
    "ingredients": {
      "type": "text"
    },
    "nutritional_values": {
      "type": "text"
    },
    "allergens": {
      "type": "text"
    },
    "consumption_date": {
      "type": "integer"
    },
    "raw_name": {
      "type": "string"
    },
    "weight": {
      "type": "decimal"
    },
    "generic_product": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::generic-product.generic-product"
    },
    "product_url": {
      "type": "string"
    },
    "product_id": {
      "type": "string",
      "unique": true
    },
    "raw_image_url": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "quantity_multiple": {
      "type": "integer"
    },
    "quantity_unit_name": {
      "type": "string"
    },
    "conservation_mode": {
      "type": "string"
    },
    "is_matched": {
      "type": "boolean"
    },
    "product_sku": {
      "type": "string"
    }
  }
}
