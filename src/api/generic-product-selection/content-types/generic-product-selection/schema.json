{
  "kind": "collectionType",
  "collectionName": "generic_product_selections",
  "info": {
    "singularName": "generic-product-selection",
    "pluralName": "generic-product-selections",
    "displayName": "Generic Product Selection",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "quantity": {
      "type": "integer",
      "required": true
    },
    "brand": {
      "type": "string"
    },
    "generic_grocery_list": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::generic-grocery-list.generic-grocery-list"
    },
    "generic_product": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::generic-product.generic-product"
    },
    "tags_list": {
      "type": "customField",
      "options": [
        "french",
        "organic",
        "local",
        "best_price",
        "best_product"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "weight": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "weight_unit": {
      "type": "enumeration",
      "enum": [
        "u",
        "g",
        "ml"
      ],
      "required": true
    }
  }
}
